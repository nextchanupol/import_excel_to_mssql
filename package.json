{
    "name": "import_job",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "npm run build && node dist/server.js",
        "dev": "nodemon",
        "build": "rimraf ./dist && tsc",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "prettier:watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn prettier:format && yarn lint",
            "pre-push": ""
        }
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dayjs": "^1.11.5",
        "exceljs": "^4.3.0",
        "express": "^4.18.1",
        "joi": "^17.6.0",
        "multer": "^1.4.5-lts.1",
        "rootpath": "^0.1.2",
        "sequelize": "^6.21.4",
        "tedious": "^15.1.0",
        "typescript": "^4.8.2",
        "uuid": "^9.0.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/joi": "^17.2.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.7.15",
        "@types/sequelize": "^4.28.14",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "nodemon": "^2.0.19",
        "onchange": "^7.1.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1"
    }
}
